  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:npm": "concurrently -n FRONTEND,BACKEND -c green,blue \"npm run tauri\" \"npm run start:npm\"",
    "dev:bun": "concurrently -n FRONTEND,BACKEND -c green,blue \"bun run tauri\" \"bun run start:bun\"",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tauri": "tauri dev",
    "start:bun": "cd backend/Server && bun dev",
    "start:npm": "cd backend/Server && npm run dev",
    "create:superadmin": "cd backend/Server && node scripts/create-superadmin.js",
    "migrate:branch-maps": "cd backend/Server && node scripts/add-branch-map-fields.js",
    "install:backend:bun": "cd backend/Server && bun install",
    "install:backend:npm": "cd backend/Server && npm install"
  },
